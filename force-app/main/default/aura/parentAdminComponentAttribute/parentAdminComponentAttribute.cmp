<aura:component implements="flexipage:availableForAllPageTypes" access="global" >
	<aura:attribute name="name1" type="String" />
    <aura:attribute name="name2" type="String" />  
    
    <lightning:input type="text" label="Enter firstName" value="{!v.name1}"/><br/><br/>
    <lightning:input type="text" label="Enter lastName" value="{!v.name2}"/><br/><br/>
    <c:childAdminComponentAttribute firstName="{!v.name1}" lastName="{!v.name2}"/><br/><br/>
</aura:component>